<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="?android:attr/colorControlHighlight">
    <item android:id="@android:id/mask">
        <inset android:insetLeft="4dp" android:insetRight="4dp" android:insetTop="6dp" android:insetBottom="6dp">
            <shape android:shape="rectangle">
                <corners android:radius="12dp"/>
                <solid android:color="?android:attr/colorControlHighlight"/>
            </shape>
        </inset>
    </item>
    <item android:id="@android:id/background">
        <inset android:insetLeft="4dp" android:insetRight="4dp" android:insetTop="6dp" android:insetBottom="6dp">
            <shape android:shape="rectangle">
                <solid android:color="@color/settingslib_colorSurface"/>
                <corners android:radius="12dp"/>
            </shape>
        </inset>
    </item>
</ripple>
